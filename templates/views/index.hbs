		$(document).ready(function () {
			$("#contact-form").on("submit", function () {

				var that = $(this),
					url = that.attr("action"),
					type = that.attr("method"),
					data = {};

				that.find("[name]").each(function (index, value) {
					var that = $(this),
						name = that.attr("name"),
						value = that.val();

					data[name] = value;
				});

				$.ajax({
					url: url,
					type: type,
					data: data,
					success: function (response) {
						console.log(response);
					}
				});
				$('#contact-form').each(function () {
					this.reset();
				});

				$('#success').html(
					"<p class='text-center text-white mb-0'>Thank you!</p><p class='text-center text-white'>Your form has been submitted successfully</p>"
				);
				return false;
			});

		});
